<% for(var i = 0; i < users.length; i++) { %>
    <tr>
        <td><%= i + 1 %></td>
        <td><%= users[i].name %></td>
        <td><%= users[i].email %></td>
        <td><%= users[i].sexo %></td>
        <td><%= users[i].status %></td>
        <td>
            <a href="/update-user?id=<%= users[i]._id%>" class="btn btn-primary">
                <span class="text-gradient"> Atualizar<i class="fas fa-pencil-alt"></i></span>
            </a>
            <a class="btn btn-danger" data-id=<%= users[i]._id%>
                <span class="text-gradient">Deletar<i class="fas fa-times"></i></span>
            </a>
        </td>
    </tr>
    <% } %>