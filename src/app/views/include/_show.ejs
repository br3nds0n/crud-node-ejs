<% for(var i = 0; i < users.length; i++) { %>
<tr>
    <td><%= i + 1 %></td>
    <td><%= users[i].name %></td>
    <td><%= users[i].email %></td>
    <td><%= users[i].sexo %></td>
    <td><%= users[i].status %></td>
    <td>
        <a href="/update-user?id=<%= users[i]._id%>" class="btn btn-primary update">
            Atualizar
        </a>
        <a class="btn btn-danger delete" data-id=<%= users[i]._id%> >
           Deletar
        </a>
    </td>
</tr>
<% } %>